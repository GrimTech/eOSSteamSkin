checkbox.styles
{
	styles
	{
		CheckButton
		{
			font-family=basefont
			font-size=14
			font-weight=400
			textcolor="Label"
			image="graphics/eos/controls/checkbox/unchecked"
		}
		CheckButton:selected
		{
			image="graphics/eos/controls/checkbox/checked"
		}
		CheckButton:disabled
		{
			textcolor="TextDisabled"
			image="graphics/eos/controls/checkbox/unchecked"
		}
		
		CheckButton [$OSX]
		{
			image="graphics/mac/controls/checkbox/unchecked/normal"
		}
		CheckButton:active [$OSX]
		{
			image="graphics/mac/controls/checkbox/unchecked/pressed"
		}
		CheckButton:disabled [$OSX]
		{
			image="graphics/mac/controls/checkbox/unchecked/disabled"
		}
		CheckButton:selected [$OSX]
		{
			image="graphics/mac/controls/checkbox/checked/normal"
		}
		CheckButton:selected:active [$OSX]
		{
			image="graphics/mac/controls/checkbox/checked/pressed"
		}
		CheckButton:selected:disabled [$OSX]
		{
			image="graphics/mac/controls/checkbox/checked/disabled"
		}
		
		IndeterminateCheckButton
		{
			font-family=basefont
			font-size=14
			font-weight=400
			textcolor="Label"
			bgcolor=none
			image="graphics/eos/controls/checkbox/unchecked"
		}

		IndeterminateCheckButton [$OSX]
		{
			image="graphics/mac/controls/checkbox/indeterminate/normal"
		}
		IndeterminateCheckButton:active [$OSX]
		{
			image="graphics/mac/controls/checkbox/indeterminate/pressed"
		}
		IndeterminateCheckButton:disabled [$OSX]
		{
			image="graphics/mac/controls/checkbox/indeterminate/disabled"
		}

		ListPanelCheckBox
		{
			font-family=basefont
			font-size=14
			font-weight=400
			textcolor="Label"
			image="graphics/eos/controls/checkbox/unchecked"
			inset="0 5 0 0"
		}
		ListPanelCheckBox:selected
		{
			image="graphics/graphics/eos/controls/checkbox/checked"
		}
		ListPanelCheckBox:disabled
		{
			textcolor="TextDisabled"
			image="graphics/graphics/eos/controls/checkbox/unchecked"
		}

		ListPanelCheckBox [$OSX]
		{
			image="graphics/mac/controls/checkbox/unchecked/normal"
		}
		ListPanelCheckBox:active [$OSX]
		{
			image="graphics/mac/controls/checkbox/unchecked/pressed"
		}
		ListPanelCheckBox:disabled [$OSX]
		{
			textcolor="TextDisabled"
			image="graphics/mac/controls/checkbox/unchecked/disabled"
		}
		ListPanelCheckBox:selected [$OSX]
		{
			image="graphics/mac/controls/checkbox/checked/normal"
		}
		ListPanelCheckBox:selected:active [$OSX]
		{
			image="graphics/mac/controls/checkbox/checked/pressed"
		}
		ListPanelCheckBox:selected:disabled [$OSX]
		{
			textcolor="TextDisabled"
			image="graphics/mac/controls/checkbox/checked/disabled"
		}
	}
}
