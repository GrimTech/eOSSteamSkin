navigation.styles
{
	styles
	{
		BackButton
		{
			bgcolor=none
			render_bg={}
			image="graphics/elementary/light/navigation/back"
			inset="-2 4 6 4"
			padding=0
		}
		BackButton:disabled
		{
			image="graphics/elementary/light/navigation/back_disabled"
		}
		BackButton:active
		{
			render_bg
			{
				0="image(x0, y0, x1, y1, graphics/elementary/_/controls/buttons/headerbar/active_bg)"
			}
		}

		ForwardButton
