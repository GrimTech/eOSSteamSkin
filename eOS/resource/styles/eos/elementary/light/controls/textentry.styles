textentry.styles
{
	styles
	{
		TextEntry
		{
			font-family=basefont
			font-size=14
			font-weight=400
			textcolor="eOSTextFieldText"
			selectedtextcolor="eOSTextFieldSelectedText"
			selectedbgcolor="eOSTextFieldSelectedBG"
			shadowtextcolor="eOSTextFieldCursor"

			inset-left=10
			inset-right=10

			render_bg
			{
				0="image(x0, y0, x0 + 5, y0 + 5, graphics/elementary/light/controls/textentry/normal/tl)"
				1="image(x1 - 5, y0, x1, y0 + 5, graphics/elementary/light/controls/textentry/normal/tr)"
				2="image(x0, y1 - 5, x0 + 5, y1, graphics/elementary/light/controls/textentry/normal/bl)"
				3="image(x1 - 5, y1 - 5, x1, y1, graphics/elementary/light/controls/textentry/normal/br)"
				4="image_tiled(x0 + 5, y0, x1 - 5, y0 + 5, graphics/elementary/light/controls/textentry/normal/t)"
				5="image_scale(x0, y0 + 5, x0 + 5, y1 - 5, graphics/elementary/light/controls/textentry/normal/l)"
				6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/elementary/light/controls/textentry/normal/r)"
				7="image_tiled(x0 + 5, y1 - 5, x1 - 5, y1, graphics/elementary/light/controls/textentry/normal/b)"
				8="image_scale(x0 + 5, y0 + 5, x1 - 5, y1 - 5, graphics/elementary/light/controls/textentry/normal/m)"
			}	 
		}
		TextEntry:disabled
