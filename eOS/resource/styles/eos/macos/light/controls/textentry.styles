textentry.styles
{
	styles
	{
		TextEntry
		{
			font-family=basefont
			font-size=14
			font-weight=400
			textcolor="eOSTextFieldText"
			selectedtextcolor="eOSTextFieldSelectedText"
			selectedbgcolor="eOSTextFieldSelectedBG"
			shadowtextcolor="eOSTextFieldCursor"

			inset-left=10
			inset-right=10

			render_bg
			{
                0="image(x0, y0, x0 + 5, y0 + 5, graphics/macos/light/controls/textentry/tl)"
                1="image(x1 - 5, y0, x1, y0 + 5, graphics/macos/light/controls/textentry/tr)"
                2="image(x0, y1 - 5, x0 + 5, y1, graphics/macos/light/controls/textentry/bl)"
                3="image(x1 - 5, y1 - 5, x1, y1, graphics/macos/light/controls/textentry/br)"
                4="image_scale(x0 + 5, y0, x1 - 5, y0 + 5, graphics/macos/light/controls/textentry/t)"
                5="image_scale(x0, y0 + 5, x0 + 5, y1 - 5, graphics/macos/light/controls/textentry/l)"
                6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/macos/light/controls/textentry/r)"
                7="image_scale(x0 + 5, y1 - 5, x1 - 5, y1, graphics/macos/light/controls/textentry/b)"
                8="image_scale(x0 + 5, y0 + 5, x1 - 5, y1 - 5, graphics/macos/light/controls/textentry/m)"
			}	 
		}
		TextEntry:disabled
		{
			render_bg
			{
                0="image(x0, y0, x0 + 5, y0 + 5, graphics/macos/light/controls/textentry/disabled/tl)"
                1="image(x1 - 5, y0, x1, y0 + 5, graphics/macos/light/controls/textentry/disabled/tr)"
                2="image(x0, y1 - 5, x0 + 5, y1, graphics/macos/light/controls/textentry/disabled/bl)"
                3="image(x1 - 5, y1 - 5, x1, y1, graphics/macos/light/controls/textentry/disabled/br)"
                4="image_scale(x0 + 5, y0, x1 - 5, y0 + 5, graphics/macos/light/controls/textentry/disabled/t)"
                5="image_scale(x0, y0 + 5, x0 + 5, y1 - 5, graphics/macos/light/controls/textentry/disabled/l)"
                6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/macos/light/controls/textentry/disabled/r)"
                7="image_scale(x0 + 5, y1 - 5, x1 - 5, y1, graphics/macos/light/controls/textentry/disabled/b)"
                8="image_scale(x0 + 5, y0 + 5, x1 - 5, y1 - 5, graphics/macos/light/controls/textentry/disabled/m)"
			}
		}
		TextEntry:focus
		{
			render
			{
                0="image(x0 - 2, y0 - 2, x0 + 6, y0 + 6, graphics/macos/light/controls/textentry/focus/tl)"
                1="image(x1 - 6, y0 - 2, x1 + 2, y0 + 6, graphics/macos/light/controls/textentry/focus/tr)"
                2="image(x0 - 2, y1 - 6, x0 + 6, y1 + 2, graphics/macos/light/controls/textentry/focus/bl)"
                3="image(x1 - 6, y1 - 6, x1 + 2, y1 + 2, graphics/macos/light/controls/textentry/focus/br)"
                4="image_scale(x0 + 6, y0 - 2, x1 - 6, y0 + 6, graphics/macos/light/controls/textentry/focus/t)"
                5="image_scale(x0 - 2, y0 + 6, x0 + 6, y1 - 6, graphics/macos/light/controls/textentry/focus/l)"
                6="image_scale(x1 - 6, y0 + 6, x1 + 2, y1 - 6, graphics/macos/light/controls/textentry/focus/r)"
                7="image_scale(x0 + 6, y1 - 6, x1 - 6, y1 + 2, graphics/macos/light/controls/textentry/focus/b)"
			}
		}

		LibrarySearch
		{
			inset-left=0
			inset-right=0
			minimum-width=140
			render_bg
			{
                0="image(x0 - 22, y0, x0 - 17, y0 + 5, graphics/macos/light/controls/textentry/tl)"
                1="image(x1 - 5, y0, x1, y0 + 5, graphics/macos/light/controls/textentry/tr)"
                2="image(x0 - 22, y1 - 5, x0 - 17, y1, graphics/macos/light/controls/textentry/bl)"
                3="image(x1 - 5, y1 - 5, x1, y1, graphics/macos/light/controls/textentry/br)"
                4="image_scale(x0 - 17, y0, x1 - 5, y0 + 5, graphics/macos/light/controls/textentry/t)"
                5="image_scale(x0 - 22, y0 + 5, x0 - 17, y1 - 5, graphics/macos/light/controls/textentry/l)"
                6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/macos/light/controls/textentry/r)"
                7="image_scale(x0 - 17, y1 - 5, x1 - 5, y1, graphics/macos/light/controls/textentry/b)"
                8="image_scale(x0 - 17, y0 + 5, x1 - 5, y1 - 5, graphics/macos/light/controls/textentry/m)"
				9="image(x0 - 16, y0 + 6, x0 - 4, y1 - 6, graphics/macos/light/icons/search)"
			}
		}
		LibrarySearch:focus
		{
			render
			{
				0="image(x0 - 24, y0 - 2, x0 - 16, y0 + 6, graphics/macos/light/controls/textentry/focus/tl)"
                1="image(x1 - 6, y0 - 2, x1 + 2, y0 + 6, graphics/macos/light/controls/textentry/focus/tr)"
                2="image(x0 - 24, y1 - 6, x0 - 16, y1 + 2, graphics/macos/light/controls/textentry/focus/bl)"
                3="image(x1 - 6, y1 - 6, x1 + 2, y1 + 2, graphics/macos/light/controls/textentry/focus/br)"
                4="image_scale(x0 - 16, y0 - 2, x1 - 6, y0 + 6, graphics/macos/light/controls/textentry/focus/t)"
                5="image_scale(x0 - 24, y0 + 6, x0 - 16, y1 - 6, graphics/macos/light/controls/textentry/focus/l)"
                6="image_scale(x1 - 6, y0 + 6, x1 + 2, y1 - 6, graphics/macos/light/controls/textentry/focus/r)"
                7="image_scale(x0 - 16, y1 - 6, x1 - 6, y1 + 2, graphics/macos/light/controls/textentry/focus/b)"
			}
		}

		FriendsSearch
		{
			inset-left=0
			inset-right=0
			render_bg
			{
				0="image(x0 - 22, y0, x0 - 17, y0 + 5, graphics/macos/light/controls/textentry/tl)"
                1="image(x1 - 5, y0, x1, y0 + 5, graphics/macos/light/controls/textentry/tr)"
                2="image(x0 - 22, y1 - 5, x0 - 17, y1, graphics/macos/light/controls/textentry/bl)"
                3="image(x1 - 5, y1 - 5, x1, y1, graphics/macos/light/controls/textentry/br)"
                4="image_scale(x0 - 17, y0, x1 - 5, y0 + 5, graphics/macos/light/controls/textentry/t)"
                5="image_scale(x0 - 22, y0 + 5, x0 - 17, y1 - 5, graphics/macos/light/controls/textentry/l)"
                6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/macos/light/controls/textentry/r)"
                7="image_scale(x0 - 17, y1 - 5, x1 - 5, y1, graphics/macos/light/controls/textentry/b)"
                8="image_scale(x0 - 17, y0 + 5, x1 - 5, y1 - 5, graphics/macos/light/controls/textentry/m)"
				9="image(x0 - 16, y0 + 6, x0 - 4, y1 - 6, graphics/macos/light/icons/search)"
			}
		}
		FriendsSearch:focus
		{
			render
			{
				0="image(x0 - 24, y0 - 2, x0 - 16, y0 + 6, graphics/macos/light/controls/textentry/focus/tl)"
                1="image(x1 - 6, y0 - 2, x1 + 2, y0 + 6, graphics/macos/light/controls/textentry/focus/tr)"
                2="image(x0 - 24, y1 - 6, x0 - 16, y1 + 2, graphics/macos/light/controls/textentry/focus/bl)"
                3="image(x1 - 6, y1 - 6, x1 + 2, y1 + 2, graphics/macos/light/controls/textentry/focus/br)"
                4="image_scale(x0 - 16, y0 - 2, x1 - 6, y0 + 6, graphics/macos/light/controls/textentry/focus/t)"
                5="image_scale(x0 - 24, y0 + 6, x0 - 16, y1 - 6, graphics/macos/light/controls/textentry/focus/l)"
                6="image_scale(x1 - 6, y0 + 6, x1 + 2, y1 - 6, graphics/macos/light/controls/textentry/focus/r)"
                7="image_scale(x0 - 16, y1 - 6, x1 - 6, y1 + 2, graphics/macos/light/controls/textentry/focus/b)"
			}
		}
		
		TextEntryURL
		{
			inset-left=20
			render_bg
			{
				0="image(x0, y0, x0 + 5, y0 + 5, graphics/macos/light/controls/textentry/tl)"
                1="image(x1 - 5, y0, x1, y0 + 5, graphics/macos/light/controls/textentry/tr)"
                2="image(x0, y1 - 5, x0 + 5, y1, graphics/macos/light/controls/textentry/bl)"
                3="image(x1 - 5, y1 - 5, x1, y1, graphics/macos/light/controls/textentry/br)"
                4="image_scale(x0 + 5, y0, x1 - 5, y0 + 5, graphics/macos/light/controls/textentry/t)"
                5="image_scale(x0, y0 + 5, x0 + 5, y1 - 5, graphics/macos/light/controls/textentry/l)"
                6="image_scale(x1 - 5, y0 + 5, x1, y1 - 5, graphics/macos/light/controls/textentry/r)"
                7="image_scale(x0 + 5, y1 - 5, x1 - 5, y1, graphics/macos/light/controls/textentry/b)"
                8="image_scale(x0 + 5, y0 + 5, x1 - 5, y1 - 5, graphics/macos/light/controls/textentry/m)"
				9="image(x0 + 6, y0 + 6, x0 + 18, y0 + 18, graphics/_/light/icons/web/web)"
			}  
		}
		URLStatusImage
		{
			render
			{
				0="image_scale(x0, y0, x1, y1, graphics/macos/light/controls/textentry/m)"
				1="image(x0, y0, x1, y1, graphics/_/light/icons/web/secure)"
			}
		}
	}
}
